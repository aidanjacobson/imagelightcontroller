{
    "name": "Image Light Controller",
    "start_url": "https://aidanjacobson.github.io/imagelightcontroller/",
    "display": "standalone",
    "share_target": {
        "action": "https://aidanjacobson.github.io/imagelightcontroller/#light.aidan_s_room_lights",
        "method": "POST",
        "enctype": "multipart/form-data",
        "params": {
            "files": [
                {
                    "name": "images",
                    "accept": ["image/*"]
                }
            ]
        }
    },
    "icons": [{
        "src": "https://www.pngfind.com/pngs/m/170-1700251_lightbulb-icon-png-light-favicon-transparent-png.png",
        "sizes": "192x192 512x512"
    }]
}